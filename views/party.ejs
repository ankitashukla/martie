<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Martie · SDSLabs</title>
    <link rel="shortcut icon" href="/images/logo.png">
    <link rel="stylesheet" href="/stylesheets/pure.min.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
  </head>
  <body>
    <div class="party content" style="background-color:white;">
      <div id="navbar" class="pure-u-1 pure-menu pure-menu-open pure-menu-horizontal">
        <ul>
          <li id="topbar-logo-div"><a href="/"><img id="logo-image" src="/images/logo.png" alt="">Martie</a></li>
          <li id="topbar-url"><a id="partyurl" data-admin="<%=admin%>" data-party="<%= urlname %>" href="/party/<%=urlname%>">martie.sdslabs.co.in/party/<%= urlname %></a></li>
        </ul>
      </div>
      <div id="party-head">
        <div id="qr">
          <img src="https://chart.googleapis.com/chart?cht=qr&chs=300x300&chl=http://martie.sdslabs.co.in/party/<%=urlname%>" alt="">
        </div>
        <% if (admin === true) {%>
          <script src="http://www.youtube.com/player_api"></script>
          <div id="player"></div>
        <% } %>
      </div>
      <div class="pure-u-1-4 list">
        <h2>QUEUE</h2>
        <ul id="queue">
          <!--<% tracks.forEach(function(track){ %>
          <li class="song" data-id="<%= track.split('|')[0] %>" data-title="<%= track.split('|')[1] %>">
            <span class="name"><%= track.split('|')[1] %></span><br>
          </li>
          <% }) %>-->
        </ul>
      </div>
      <div class="pure-u-1-4 list">
        <h2>SUGGESTIONS</h2>
        <ul id="suggestions"></ul>
      </div>
      <div class="pure-u-1-4 list">
        <h2>SEARCH</h2>
        <form><input type="text" class="pure-input-1" placeholder="Search" id="party-search"></form>
        <div id="loader">
          <img src="/images/loader.gif" alt="">
        </div>
        <ul id="search-list">
        </ul>
      </div>
      <div id="footer">
          Team SDSLabs · 2013 · <a href="//github.com/sdslabs/martie">github.com/sdslabs/martie</a>
      </div>
    </div>
    <script src="/javascripts/jquery.min.js"></script>
    <script src="/javascripts/imagesLoaded.min.js"></script>
    <script src="/javascripts/masonry.min.js"></script>
    <script src="/javascripts/main.js"></script>
    <script src="/javascripts/AppWarpClientSDK.js"></script>
    <script src="/javascripts/party.js"></script>
    <script src="/javascripts/Listeners.js"></script>
    <script src="/javascripts/shp.js"></script>
  </body>
</html>
<script>
var tracks=<%- tracks=JSON.stringify(tracks) %>
</script>
